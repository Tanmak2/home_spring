<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.EmpMapper">

<select id="select_venture" resultType="com.example.demo.VO.venture">
	SELECT * FROM venture
</select>

<select id="selectVentureFindByColumnData" resultType="com.example.demo.VO.venture">
	SELECT * FROM venture WHERE company LIKE #{data}
</select>

<select id="select_venture_list" resultType="com.example.demo.VO.venture">
	SELECT * FROM venture LIMIT #{index},20
</select>
</mapper>